{
  "expo": {
    "name": "Water Remainder",
    "slug": "Water-Remainder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#007ad2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    "notification": {
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "android": {
        "sound": "./assets/sounds/Ring.mp3"
      }
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#007ad2"
      },
      "package": "com.ayush01010101.WaterRemainder"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2e4f4d1b-2f17-42b4-9263-ee80a6c6242c"
      }
    },
    "owner": "ayush01010101"
  }
}
